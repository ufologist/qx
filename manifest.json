{
  "first_page": "$(AppDir)/qx/qx.html",
  "application_title": "纷享销客 企信独立版",
  "application_shortname": "",
  "use_grit_package": true,
  "icon_path": "",
  "use_node": true,
  "version": "0.4.0",
  "locale": "zh-CN",
  "multiple_process": false,
  "launch_node_in_all_pages": false,
  "load_node_manually": false,
  "disable_async_node_apis": false,
  "remote_debugging_port": 65432,
  "disable_debug_log": true,
  "quit_after_main_window_closed": false,
  "cache_path": "",
  "npapi_plugin_directory": "",
  "disable_ime_composition": false,
  "extensions": [
    "hex_dialog",
    "hex_sleep",
    "hex_shortcut",
    "hex_icon",
    "hex_custom_protocol",
    "hex_registry",
    "hex_file_association"
  ],
  "extension_path": "",
  "single_instance": true,
  "window_class_name": "qx",
  "form": {
    "style": "standard",
    "plain": false,
    "system_buttons": true,
    "transmission_color": "none",
